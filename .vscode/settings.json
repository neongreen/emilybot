{
  "python.venvPath": "",
  "python.pythonPath": ".venv/bin/python",
  "[python]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[toml]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[yaml]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[json]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[markdown]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "files.exclude": {
    "**/.git": true,
    ".pytest_cache": true,
    "**/__pycache__": true,
    "**/.jj": true,
    "**/*.egg-info": true
  },
  "deno.enable": true
}
