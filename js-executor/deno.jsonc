{
  "compilerOptions": {
    "strict": true,
    "lib": ["ES2022", "DOM", "deno.ns"]
  },
  "imports": {
    "quickjs-emscripten": "npm:quickjs-emscripten@^0.31.0",
    "quickjs-emscripten-sync": "npm:quickjs-emscripten-sync@^1.5.2",
    "zod": "npm:zod@^4.0.14"
  },
  // Put node modules into js-executor/ so that --allow-read for the cache folder is not needed
  "nodeModulesDir": "auto",
  "lint": {
    "rules": {
      "exclude": ["no-explicit-any"]
    }
  }
}
